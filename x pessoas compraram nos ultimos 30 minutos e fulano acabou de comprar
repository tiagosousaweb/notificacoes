        nome.sort(() => Math.random() - 0.5);
        var i = 0, qtd = nome.length;
        setInterval(function doThis() {
            if (i == 11) return; // somente 11 eventos ser√£o disparados
            if (i == 1 || i == 8 ||  i == 10) {
                toastr.info((qtd + (i * 3)) + ' pessoas compraram nos ultimos 30min.', {
